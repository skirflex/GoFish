<Window x:Class="GoFish.EndGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoFish"
        mc:Ignorable="d"
        Title="Game over" Height="239" Width="484" ResizeMode="NoResize" Icon="/icon fishgame.jpg">
    <Grid RenderTransformOrigin="0.506,0.556" Height="223" Width="484">
        <Label Content="Game over" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Ink free" FontSize="26" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.658,0.579" Width="464" Height="39"/>
        <Label x:Name="GOTb" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontFamily="Ink free" FontSize="22" Margin="54,64,54,86" BorderBrush="White" />
        <Button FontFamily="Ink free" FontSize="22" Content="Play again" HorizontalAlignment="Left" Margin="54,137,0,0" VerticalAlignment="Top" Height="45" Width="171" RenderTransformOrigin="0.111,0.445" Click="Button_Click"/>
        <Button FontFamily="Ink free" FontSize="22" Content="Close" HorizontalAlignment="Left" Margin="259,137,0,0" VerticalAlignment="Top" Height="45" Width="171" Click="Button_Click_1"/>
    </Grid>
</Window>
