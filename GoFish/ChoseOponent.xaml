        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoFish"
        mc:Ignorable="d"
        Title="Chose Oponent" Height="209" Width="150" Icon="/icon fishgame.jpg" ResizeMode="NoResize">
    <Grid Margin="0,0,0,3">
        <StackPanel Name="stackPanel" Margin="0,0,0,-9" >
            <Button Content="Computer #1" Margin="10" FontFamily="Ink free" FontSize="16" Click="Button_Click"/>
            <Button Content="Computer #2" Margin="10" FontFamily="Ink free" FontSize="16" Click="Button_Click"/>
            <Button Content="Computer #3" Margin="10" FontFamily="Ink free" FontSize="16" Click="Button_Click"/>
            <Button Content="Computer #4" Margin="10" FontFamily="Ink free" FontSize="16" Click="Button_Click"/>

        </StackPanel>

    </Grid>
</Window>
